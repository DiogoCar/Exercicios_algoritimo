package exercicio_fix;

import javax.swing.JOptionPane;
/**
 *
 * @author Discente
 */
public class Exercicio_fix {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here

    
    double n1,Elv2,Rz2;
        
        String sn1;
   
        sn1=JOptionPane.showInputDialog(null, "Q3: Digite o primeiro valor: ");
        
        n1 = Double.parseDouble(sn1);
     
       
       Elv2= n1 * n1;
       
       Rz2= Math.sqrt(n1);
       
       JOptionPane.showMessageDialog(null,"O quadrado é: " + Elv2);
       
       JOptionPane.showMessageDialog(null,"A raiz quadrada é: " + Rz2);
    }
    
    }
     
